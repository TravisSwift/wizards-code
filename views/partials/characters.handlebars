<!--BEGINNING LOAD CHARACTER TRAY-->
<div class="saved_characters border border-black border-solid border-2 text-center">
    <h2> Saved Characters</h2>
    <ol>
        {{#each characters as |character|}}
        <li>
            <a href="/character/{{character.id}}" target="_blank">{{character.name}}</a>
        </li>
        {{/each}}
    </ol>
</div>
<!--END LOAD CHARACTER TRAY-->